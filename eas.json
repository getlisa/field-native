{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "http://localhost:4000/api",
        "EXPO_PUBLIC_COPILOT_BASE_URL": "http://localhost:3000"
      }
    },
    "preview": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://techcopilot-core.justclara.ai/api",
        "EXPO_PUBLIC_COPILOT_BASE_URL": "https://techcopilot-assistant.justclara.ai"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "apk"
      },
      
      "environment": "production"
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "appleId": "sriram.kumar@zentrades.pro",
        "ascAppId": "6757388242",
        "appleTeamId": "TS3BFX5G72"
      }
    },
    "production": {
      "ios": {
        "appleId": "sriram.kumar@zentrades.pro",
        "ascAppId": "6757388242",
        "appleTeamId": "TS3BFX5G72"
      }
    }
  }
}
